<span class="kt">bool</span> <span class="nf">is_prime_number</span><span class="p">(</span><span class="n">string</span> <span class="n">numbers</span><span class="p">,</span> <span class="kt">int</span> <span class="n">prime_number</span><span class="p">)</span> <span class="p">{</span>
